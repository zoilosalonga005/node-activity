{
	"name": "npm-lockfile",
	"version": "2.0.5",
	"description": "Safely generate an npm lockfile and output it to the filename of your choice.",
	"bin": "./bin.js",
	"scripts": {
		"prepublishOnly": "safe-publish-latest",
		"prepublish": "not-in-publish || npm run prepublishOnly",
		"version": "auto-changelog && git add CHANGELOG.md",
		"postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\"",
		"lint": "eslint --ext=js,mjs .",
		"pretest": "npm run lint",
		"test": "npm run tests-only",
		"tests-only": "nyc tape 'test/**'",
		"posttest": "aud --production"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ljharb/npm-lockfile.git"
	},
	"keywords": [
		"npm",
		"package-lock",
		"npm-shrinkwrap",
		"shrinkwrap",
		"lockfile"
	],
	"author": "Jordan Harband <ljharb@gmail.com>",
	"funding": {
		"url": "https://github.com/sponsors/ljharb"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ljharb/npm-lockfile/issues"
	},
	"homepage": "https://github.com/ljharb/npm-lockfile#readme",
	"dependencies": {
		"colors": "=1.4.0",
		"find-package-json": "^1.2.0",
		"fs-copy-file": "^2.1.2",
		"node-cleanup": "^2.1.2",
		"object-inspect": "^1.12.0",
		"rimraf": "^3.0.2",
		"semver": "^6.3.0",
		"tmp": "^0.1.0",
		"util.promisify": "^1.1.1",
		"yargs": "^14.2.3"
	},
	"engines": {
		"node": ">= 6"
	},
	"devDependencies": {
		"@ljharb/eslint-config": "^20.2.3",
		"aud": "^2.0.0",
		"auto-changelog": "^2.4.0",
		"eslint": "=8.8.0",
		"nyc": "^10.3.2",
		"safe-publish-latest": "^2.0.0",
		"tape": "^5.5.2"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false,
		"hideCredit": true
	}
}
